public class Main {
    public static void main(String[] args) {
        int[] array = new int[20];
        for (int i = 0; i < 20; i++) {
            array[i] = (int) (Math.random() * (100) + 0);
        }
        for (int b = 0; b < 20; b++) {
            if (array[b] % 2 != 0) {
                System.out.println(array[b] + " нечет");
            } else
                System.out.println(array[b] + " чет");
        }

    }
}
